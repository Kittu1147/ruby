<%= form_tag  sregister_index_path do  %>
				<p>
					<%= label_tag :register_number %><br>
					<%= text_field_tag :rno %>
				</p>
			
				<p>
					<%= label_tag :name %><br>
					<%= text_field_tag :name %>
				</p>
			
				<p>
					<%= label_tag :cgpa %><br>
					<%= text_field_tag :cgpa %>
				</p>
			
				<p>
					<%= label_tag :branch %><br>
					<%= text_field_tag :branch %>
				</p>
			
				<p>
					<%= label_tag :contact_number %><br>
					<%= text_field_tag :cno %>
				</p>
			
				<p>
					<%= label_tag :mail_id %><br>
					<%= email_field_tag :mail %>
				</p>

				<p>
					<%= label_tag :password %><br>
					<%= password_field_tag :pword1 %>
				</p>
			
				<p>
					<%= label_tag :confirm_password %><br>
					<%= password_field_tag :pword2 %>
				</p>
				<p>
					<%= submit_tag "REGISTER" %>
				</p>
<% end %>
